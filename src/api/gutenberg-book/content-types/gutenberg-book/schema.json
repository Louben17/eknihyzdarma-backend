{
  "kind": "collectionType",
  "collectionName": "gutenberg_books",
  "info": {
    "singularName": "gutenberg-book",
    "pluralName": "gutenberg-books",
    "displayName": "Gutenberg Book",
    "description": "Anglick√© e-knihy z Project Gutenberg"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "author": {
      "type": "string"
    },
    "coverUrl": {
      "type": "string"
    },
    "epubUrl": {
      "type": "string"
    },
    "gutenbergId": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "subjects": {
      "type": "json"
    },
    "category": {
      "type": "string"
    },
    "gutenbergDownloads": {
      "type": "integer",
      "default": 0
    },
    "downloads": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "content-manager": {
          "visible": false
        }
      }
    }
  }
}
